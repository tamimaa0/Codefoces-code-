#include<bits/stdc++.h>
#define ll long long int
#define cinn(v) for(int i=0; i<n; i++)cin>>v[i]
using namespace std;

int main(){

    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;

    t =1 ;
    cin >> t ;

    while (t--){

        ll n;
        cin>>n;

        //vector<ll>v(n);

        vector<pair<ll, ll>>v(n);

        for(int i=0; i<n; i++)
        {
            cin>>v[i].first;
            v[i].second= i+1;
        }

        sort(v.begin(), v.end());

        ll sum =1;
        bool flag = true ;

        for(int i = 1 ; i<n; i++){

            if(v[i].first==v[i-1].first)
                sum++;
            else
            {  // cout<<i<<endl;
                if(sum==1)
                    flag = false;
                sum = 1;
            }


        }

        //cout<<sum<<endl;

        if(sum==1)
            flag = 0;

        if(flag){
             vector<vector<ll>>ans;
             vector<ll>temp;
            for(int i = 0 ; i<n; i++){

                if(v[i].first==v[i+1].first){
                    temp.push_back(v[i].second);
                }else {
                    temp.push_back(v[i].second);
                    ans.push_back(temp);
                    temp.clear();
                }



            }

            ans.push_back(temp);



            for(auto u:ans){
                if(u.size()&1){
                     ll mid = u.size()/2;
                    swap(u[0], u[mid]);
                }
                for(int i= u.size()-1; i>=0; i--){
                    cout<<u[i]<<" ";
                }

            }
            cout<<endl;

        }else
            cout<<-1<<endl;

    }

    return 0;
}
