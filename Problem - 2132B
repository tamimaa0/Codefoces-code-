#include<bits/stdc++.h>
#define ll long long int
#define cinn(v) for(int i=0; i<n; i++)cin>>v[i]
using namespace std;

const int N = 1e9+10;

void solve(vector<ll> &ans , int i , ll n){

    if(i > 18 )
        return ;

   // ll x = 1LL + pow(10LL, i) ;
        ll x = pow(10LL, i) ;

    if(x +1 >n) return ;


    ll num = n / (x+1) ;

    //cout<<x+1<< " "<< num<<endl;

    if(n% (x+1) ==0){
        ans.push_back(num);
    }

    solve(ans, i+1, n);
}

int main(){

    ios::sync_with_stdio(false);
    cin.tie(0);


    int t;

    t =1 ;
    cin >> t ;

    while (t--){

        ll n;
        cin>>n;

        vector<ll>ans ;

        solve(ans, 1, n);

        sort(ans.begin(), ans.end());

        cout<<ans.size()<<endl;

        for(auto u :ans )cout<<u <<" ";

        cout<<endl;



    }
      //main();

    return 0;
}
