#include<bits/stdc++.h>
#define ll long long int
#define cinn(v) for(int i=0; i<n; i++)cin>>v[i]
using namespace std;

void avai(ll n, vector<bool>&a){

    while(n>0){
        a[n%10]= true;
        n/=10;
    }
}


int main(){

    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;

    t =1 ;
    cin >> t ;

    while (t--){

        ll n;
        cin>>n;

        vector<bool>a(10, false);
        //avai(n, a);

       // for(auto u: a) cout<<u<<" ";cout<<endl;

        while(1){


            avai(n, a);
            bool flag =true;
            for(int i =1 ; i<=9; i++){
                    if(a[i]){
                            //cout<<i<<" ";
                       if(n%i!=0 )
                        flag = false ;
                    }

            }
            //cout<<endl;
            //cout<<n<<" "<<flag<<endl;

            if(flag)
                break;
            n++;
            //a.clear();
            for(int i=1; i<=9; i++)a[i]=0;
        }

        cout<<n<<endl;

    }

    return 0;
}
