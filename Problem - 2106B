#include <bits/stdc++.h>
#define ll long long int
#define db double

const int N=1e6+10;
using namespace std;

vector<bool>v(N,0);

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    //memset(dp,-1, sizeof(dp));


    int t=1;
    cin >> t;



    while (t--)
    {
        int n, m;
        cin>>n>>m;

        for(int i=0 ;i<n; i++){
            if(i!=m)
            cout<<i<<" ";
        }
        if(m<n)
        cout<<m<<endl;

    }

    return 0;

    }
