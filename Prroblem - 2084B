#include <bits/stdc++.h>
#define ll long long int
#define db double

const int N=1e6+10;
using namespace std;

vector<bool>v(N,0);


int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    //memset(dp,-1, sizeof(dp));


    int t=1;
    cin >> t;



    while (t--)
    {

       ll n;
       cin>>n;

       vector<ll>v(n);

       for(int i=0; i<n; i++){
        cin>>v[i];
       }

       sort(v.begin(), v.end());

       stack<ll>st;



       for(int i=1; i<n; i++){
            if(v[i]%v[0]==0)
                st.push(v[i]);
       }

       ll gc=1;

         if(!st.empty())
            gc=st.top();

        while(!st.empty()){

            gc=__gcd(gc, st.top());
            st.pop();
        }

        if(gc==v[0]or v[0]==v[1])
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;

    }

    return 0;

    }
