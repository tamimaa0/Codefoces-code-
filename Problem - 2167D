#include <bits/stdc++.h>
#define ll long long int
#define db double

const int N=1e6+10;
const ll M = 1000000007;
using namespace std;

vector<bool>prime(N , true) ;

vector<ll>p;

void fiill(){
    int i =0 ;

    while(p.size()<100){
        if(prime[i]){
            p.push_back(i);
        }

        i++;
    }

}


void seive(){

    prime[1] = false ;
    prime[0] = false;


    for (int i = 2; i*i <= N; i++) {
        if (prime[i]) {


            for (int j = i * i; j <= N; j += i)
                prime[j] = false;
        }
    }




}

void pr_f(vector<ll>p , vector<ll>&p_f ,ll n , int i){

    if(prime[n])
    {
        p_f.push_back(n) ;
        return;
    }

    if(n%p[i] == 0){
        p_f.push_back(p[i]);
        pr_f(p,p_f, n/p[i], i);

    } else
    {
        pr_f(p,p_f, n, i+1);
    }

}



void solve(){


    int n;
    cin>>n;

    vector<ll>v(n);

    for(int i=0 ; i<n; i++)
        cin>>v[i];

    for(auto u: p){

        for(auto q :v){
            if(__gcd(u, q)==1){
                    cout<<u <<endl;
                return ;
            }
        }
    }

    cout<<-1<<endl;

}
//13:13



int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    seive();
    fiill();



    int t = 1;
    cin >> t;
    while (t--)
    {

        solve();

    }
    return 0;
    }
