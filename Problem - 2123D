#include<bits/stdc++.h>

using namespace std;


int main(){

    int t;

    cin >> t ;

    while (t--){

       int  n, m ;

       cin>> n>> m;


       vector<char>v(n);

       for(int i=0 ; i<n; i++)
        cin>>v[i];

       int count = 0;

       for(int i=0 ; i<n; i++)
       {
           if(v[i]=='1')
            count++;
       }

      if(count<=m){
        cout<<"ALICE"<<endl;
      }else if(n<2*m)
      {
          cout<<"ALICE"<<endl;
      } else

        cout<<"BOB"<<endl;


    }


}
